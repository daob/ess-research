TITLE: MTMM analysis women-small model
DATA: 
 FILE IS C:\ESS\berlin\data\women.dat;

VARIABLE: 
 NAMES ARE 
	ID	cntrynum		dweight 
	WORK MENFST MENJB 
	TEST8 TEST9 TEST10 
	TEST22 TEST23 TEST24
	SPLTADMA;

 USEVARIABLES ARE 
	WORK MENFST MENJB 
	TEST8 TEST9 TEST10 
	TEST22 TEST23 TEST24
	;

 USEOBSERVATIONS ARE 
   (dweight NE -9999 AND
	 cntrynum EQ 3 OR cntrynum EQ 22 OR
	cntrynum EQ  4 OR cntrynum EQ 12 );
 
 WEIGHT IS dweight;
 GROUPING IS cntrynum (3=CH 4=CZ 12=GR 22=SI);
 IDVARIABLE = ID;
 MISSING ARE ALL(-9999);


ANALYSIS: 
 TYPE = MISSING MEANSTRUCTURE;
 ESTIMATOR IS MLR;
 PROCESSORS=2;
 

MODEL:
   M1 BY WORK@1 MENFST@1 MENJB@1; 
   M2 BY TEST8@1 TEST9@1 TEST10@1;
   M3 BY TEST22@1 TEST23@1 TEST24@1;

   T1 BY WORK@1   TEST8 TEST22;
   T2 BY MENFST@1 TEST9 TEST23;
   T3 BY MENJB@1  TEST10 TEST24;

   WORK MENFST MENJB 
      TEST8 TEST9 TEST10
	TEST22 TEST23 TEST24;

    T1*.5 T2*.5 T3*.5 M1*.5 M2*.5 M3;

    [ T1@0 T2@0 T3@0 M1@0 M2@0 M3@0 ];

    T1 WITH T2 T3 M1@0 M2@0 M3@0;
    T2 WITH T3 M1@0 M2@0 M3@0;
    T3 WITH M1@0 M2@0 M3@0;
    M1 WITH M2@0 M3@0;
    M2 WITH M3@0;

    [	WORK MENFST MENJB 
	TEST8 TEST9 TEST10 
	TEST22 TEST23 TEST24 ];

MODEL CH:

   T1 BY WORK@1   TEST8 TEST22;
   T2 BY MENFST@1 TEST9 TEST23;
   T3 BY MENJB@1  TEST10 TEST24;

   WORK MENFST MENJB 
      TEST8 TEST9 TEST10
	TEST22 TEST23 TEST24;

    T1*.5 T2*.5 T3*.5 M1*.5 M2*.5 M3;


[   WORK*2.402 MENFST*2.081 MENJB*3.392 
      TEST8*3.022 TEST9*3.195 TEST10*2.379 
	TEST22 TEST23 TEST24];


MODEL CZ:
   T1 BY WORK@1   TEST8 TEST22;
   T2 BY MENFST@1 TEST9 TEST23;
   T3 BY MENJB@1  TEST10 TEST24;

   WORK MENFST MENJB 
      TEST8 TEST9 TEST10
	TEST22 TEST23 TEST24;

    M2 BY TEST9*0.170;

    T1*.5 T2*.5 T3*.5 M1*.5 M2*.5 M3;


[	WORK*2.517 MENFST*1.912 MENJB*3.049 
	TEST8*2.587 TEST9*3.107 TEST10*2.316 
	TEST22 TEST23 TEST24];

MODEL GR:
   T1 BY WORK@1   TEST8 TEST22;
   T2 BY MENFST@1 TEST9 TEST23;
   T3 BY MENJB@1  TEST10 TEST24;

   WORK MENFST MENJB 
      TEST8 TEST9 TEST10
	TEST22 TEST23 TEST24;

    T1*.5 T2*.5 T3*.5 M1*.5 M2*.5 M3;


[	WORK*2.316 MENFST*2.068 MENJB*2.792 
	TEST8*2.977 TEST9*3.201 TEST10*2.728 
	TEST22 TEST23 TEST24];

MODEL SI:
   T1 BY WORK@1   TEST8 TEST22;
   T2 BY MENFST@1 TEST9 TEST23;
   T3 BY MENJB@1  TEST10 TEST24;

   WORK MENFST MENJB 
      TEST8 TEST9 TEST10
	TEST22 TEST23 TEST24;

    T1*.5 T2*.5 T3*.5 M1*.5 M2*.5 M3;


[	WORK*0.860 MENFST*1.810 MENJB*3.468 
	TEST8*2.744 TEST9*3.373 TEST10*2.225 
	TEST22 TEST23 TEST24];


